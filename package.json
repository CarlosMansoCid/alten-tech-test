    "cypress": "^13.13.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "happy-dom": "^14.12.3",
    "jsdom": "^24.1.1",
    "postcss": "^8.4.40",
    "postcss-cli": "^11.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vitest": "^2.0.5"
  }
